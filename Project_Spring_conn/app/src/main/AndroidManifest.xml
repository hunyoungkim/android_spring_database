<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.project_spring_conn">

<uses-permission android:name="android.permission.INTERNET" />

<application
    android:name="com.example.project_spring_conn.AppController"
    android:allowBackup="true"
    android:icon="@drawable/ic_computer_black_24dp"
    android:label="SPRING DB 연동"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".MemberInfoActivity"></activity>
    <activity android:name=".RegisterActivity" />
    <activity android:name=".BoardUpdateActivity" />
    <activity android:name=".BoardDeleteActivity" />
    <activity android:name=".BoardWriteActivity" />
    <activity android:name=".LoginActivity">

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>